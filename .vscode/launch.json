    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "Python: builder",
            "type": "python",
            "request": "launch",
            "program": "builder/builder.py",
            "args": [
                "lexicons/lexicon.txt",
                "3",
            ],
            "console": "integratedTerminal"
        },

        {
            "name": "Python: mturk",
            "type": "python",
            "request": "launch",
            "program": "mturk/mturk.py",
            "console": "integratedTerminal",
            "args": [
                "--limit",
                "50",
                "--inputfile",
                "mturk/input-v5.yaml"
                "--outputfile",
                "mturk/output",
                "--intentcount",
                "6",
                "--linkwordscount",
                "5"
            ],
        },
    ]
}